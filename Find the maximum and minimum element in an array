import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int[] a=new int[]{1,2,3,4,5,6,};
		int max=a[0];
		int min=a[0];
		for(int i=0;i<a.length;i++)
		{
		    if(max<a[i])
		    {
		        max=a[i];
		    }
		    else if(min>a[i])
		    {
		        min=a[i];
		    }
		}
		System.out.println("Max:"+max+"\n"+"Min:"+min);
	}
}
